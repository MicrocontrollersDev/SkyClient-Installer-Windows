        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Skyclient"
        mc:Ignorable="d"
        Title="ConfirmFirstInstallWindow" Height="450" Width="800">
    <Grid>
        <Label Content="You don't have Skyclient installed" HorizontalAlignment="Left" Margin="69,140,0,0" VerticalAlignment="Top"/>
        <Button Content="Yes i want to install skyclient" HorizontalAlignment="Left" Margin="69,174,0,0" VerticalAlignment="Top" Click="Button_Click"/>
    </Grid>
</Window>
