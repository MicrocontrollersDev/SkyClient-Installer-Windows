<Application x:Class="Skyclient.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Skyclient"
             StartupUri="MainWindow.xaml"
             Startup="AppStartup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Views/Styles/Colors.xaml"/>
                <ResourceDictionary Source="Views/Styles/Labels.xaml"/>
                <ResourceDictionary Source="Views/Styles/Buttons.xaml"/>
                <ResourceDictionary Source="Views/Styles/Images.xaml"/>
                <ResourceDictionary Source="Views/Styles/Checkboxes.xaml"/>
                <ResourceDictionary Source="Views/Styles/ListBox.xaml"/>
                <ResourceDictionary Source="Views/Styles/Tabcontrol.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
